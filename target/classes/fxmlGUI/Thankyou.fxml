<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #0a2249;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlGUI.ThankYouController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="108.0" layoutY="243.0" text="Thank you for using Javelas Spell Checker" textFill="WHITE">
               <font>
                  <Font name="Segoe UI Light" size="28.0" />
               </font>
            </Label>
            <Button fx:id="checkAgain" blendMode="SCREEN" layoutX="222.0" layoutY="387.0" mnemonicParsing="false" onAction="#clickCheckAgain" prefHeight="41.0" prefWidth="257.0" text="Check another text">
               <font>
                  <Font name="Segoe UI Light" size="18.0" />
               </font>
            </Button>
            <ImageView fitHeight="221.0" fitWidth="303.0" layoutX="184.0" layoutY="42.0">
               <image>
                  <Image url="@javelascheck.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
