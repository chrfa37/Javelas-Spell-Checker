<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #124f4a;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlGUI.MistakeMenuController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="suggestions" fill="WHITE" layoutX="15.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="670.13671875">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <AnchorPane prefHeight="88.0" prefWidth="700.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="skipThis" layoutX="60.0" layoutY="29.0" mnemonicParsing="false" onAction="#clickSkipThis" prefHeight="30.0" prefWidth="150.0" text="Skip this word" />
            <Button fx:id="addToDictionary" layoutX="488.0" layoutY="29.0" mnemonicParsing="false" onAction="#clickAddToDictionary" prefHeight="30.0" prefWidth="150.0" text="Add word to dictionary" />
            <Button fx:id="skipSame" layoutX="275.0" layoutY="29.0" mnemonicParsing="false" onAction="#clickSkipSame" prefHeight="30.0" prefWidth="150.0" text="Skip this word everytime" />
         </children>
      </AnchorPane>
   </bottom>
   <top>
      <AnchorPane prefHeight="57.0" prefWidth="700.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="getSuggestionsButton" layoutX="583.0" layoutY="16.0" mnemonicParsing="false" onAction="#getSuggestions" text="Get Suggestions" />
            <Text fx:id="wrongWordPanel" fill="WHITE" layoutX="22.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="542.13671875">
               <font>
                  <Font size="18.0" />
               </font>
            </Text>
         </children></AnchorPane>
   </top>
</BorderPane>
